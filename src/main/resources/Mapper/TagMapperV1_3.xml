<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdwhNrcc.dao.v1_3.TagMapper">
 	<insert id="add" parameterType="com.sdwhNrcc.entity.v1_3.Tag">
 		insert into tag
 		<trim prefix="(" suffix=")" suffixOverrides=",">
 			<if test="temporary!=null">
 				temporary,
 			</if>
 			<if test="entityType!=null">
 				entityType,
 			</if>
 			<if test="tagStyle!=null">
 				tagStyle,
 			</if>
 			<if test="id!=null">
 				id,
 			</if>
 			<if test="userId!=null">
 				userId,
 			</if>
 			<if test="engineMask!=null">
 				engineMask,
 			</if>
 		</trim>
 		values
 		<trim prefix="(" suffix=")" suffixOverrides=",">
 			<if test="temporary!=null">
 				#{temporary},
 			</if>
 			<if test="entityType!=null">
 				#{entityType},
 			</if>
 			<if test="tagStyle!=null">
 				#{tagStyle},
 			</if>
 			<if test="id!=null">
 				#{id},
 			</if>
 			<if test="userId!=null">
 				#{userId},
 			</if>
 			<if test="engineMask!=null">
 				#{engineMask},
 			</if>
 		</trim>
 	</insert>
	<update id="edit" parameterType="com.sdwhNrcc.entity.v1_3.Tag">
		update tag set
		<trim suffixOverrides=",">
			<if test="temporary!=null">
				temporary=#{temporary},
			</if>
			<if test="entityType!=null">
				entityType=#{entityType},
			</if>
			<if test="tagStyle!=null">
				tagStyle=#{tagStyle},
			</if>
			<if test="userId!=null">
				userId=#{userId},
			</if>
			<if test="engineMask!=null">
				engineMask=#{engineMask},
			</if>
		</trim>
		where id=#{id}
	</update>
	<select id="select" resultType="com.sdwhNrcc.entity.v1_3.Tag">
		select * from tag
	</select>
	<select id="getCountById" resultType="java.lang.Integer">
		select count(*) from tag where id=#{id}
	</select>
</mapper>
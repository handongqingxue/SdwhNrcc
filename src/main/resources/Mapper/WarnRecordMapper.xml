<mapper namespace="com.sdwhNrcc.dao.WarnRecordMapper">
 	<insert id="add" parameterType="com.sdwhNrcc.entity.WarnRecord">
 		insert into warn_record
 		<trim prefix="(" suffix=")" suffixOverrides=",">
 			<if test="tagId!=null">
 				tagId,
 			</if>
 			<if test="warnType!=null">

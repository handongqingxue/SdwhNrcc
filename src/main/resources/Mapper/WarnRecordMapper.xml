 			</if>
 			<if test="rootAreaId!=null">
 				#{rootAreaId},
 			</if>
 		</trim>
 	</insert>
	<select id="getCountById" resultType="java.lang.Integer">
		select count(*) from warn_record where id=#{id}
	</select>
	<select id="queryEAList" resultType="com.sdwhNrcc.entity.WarnRecord">
		select wr.*,wt.`name` triggerName,e.`name` entityName from warn_record wr left join warn_trigger wt on wr.triggerId=wt.id left join entity e on wr.uid=e.tagId
	</select>
</mapper>